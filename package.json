{
  "name": "apolloservers-discord-bot",
  "version": "1.1.19",
  "description": "The only discord bot you will need to query your gameservers",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "compile": "tsc --build",
    "clean": "rm -rf dist/*",
    "build": "yarn clean && yarn compile",
    "dev": "NODE_ENV=development nodemon dist/index.js",
    "dev:ts": "ts-node src/index.ts",
    "gen": "graphql-codegen --config codegen.yml",
    "prepare": "husky install",
    "test": "TEST=true jest --passWithNoTests --watch",
    "test:staged": "TEST=true jest --passWithNoTests",
    "jest:clear": "jest --clearCache"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.3",
    "@types/jest": "^27.4.0",
    "@types/node-cron": "3.0.1",
    "@types/ramda": "0.27.64",
    "@types/sjcl": "1.0.30",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "commit-msg": "0.2.3",
    "dotenv": "14.3.2",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "git-commit-msg-linter": "4.0.7",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.2",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@apollo/client": "3.5.8",
    "axios": "0.25.0",
    "countries-and-timezones": "3.3.0",
    "discord.js": "13.6.0",
    "graphql": "16.3.0",
    "log4js": "6.4.1",
    "mongoose": "^6.1.8",
    "node-cron": "3.0.0",
    "ramda": "0.28.0",
    "sjcl": "1.0.8",
    "wokcommands": "1.5.3"
  }
}
